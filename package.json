{
  "name": "ai-dentist-booking-system",
  "version": "1.0.0",
  "description": "AI-powered dental appointment booking system",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm start",
    "dev:backend": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "setup:db": "cd backend && python create_tables.py",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd frontend && npm run build && serve -s build -l 3000",
    "start:backend": "cd backend && python -m uvicorn main:app --host 0.0.0.0 --port 8000"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "serve": "^14.2.1"
  },
  "keywords": [
    "dental",
    "appointment",
    "booking",
    "ai",
    "chatbot"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^4.1.0",
    "moment": "^2.30.1",
    "react-big-calendar": "^1.19.4"
  }
}
